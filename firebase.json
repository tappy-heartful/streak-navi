{
  "hosting": [
    {
      "target": "navi-prod",
      "public": "public-navi",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**/*.{html,js,css,png,jpg,jpeg,gif,webp,svg}",
          "headers": [
            { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
            { "key": "Pragma", "value": "no-cache" },
            { "key": "Expires", "value": "0" }
          ]
        }
      ]
    },
    {
      "target": "navi-test",
      "public": "public-navi",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**/*.{html,js,css,png,jpg,jpeg,gif,webp,svg}",
          "headers": [
            { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
            { "key": "Pragma", "value": "no-cache" },
            { "key": "Expires", "value": "0" }
          ]
        }
      ]
    },
