          "headers": [
            { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
            { "key": "Pragma", "value": "no-cache" },
            { "key": "Expires", "value": "0" }
          ]
        }
      ]
    },
    {
      "target": "connect-test",
      "public": "public-connect",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
